{
  "name": "anitha.restaurant",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "prettier": "prettier  --write \"./src/app/**/*.{ts,scss}\"",
    "precommit": "lint-staged",
    "start": "ng serve -o --host localhost --port 80 --proxy-config proxy.conf.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "developSTG": "ng build --configuration=STG --base-href /anitharestaurant/",
    "developSTG2": "ng build --configuration=STG2 --base-href /anitharestaurant/",
    "developUAT": "ng build --configuration=UAT --base-href /anitharestaurant/",
    "developPRD": "ng build --prod --configuration=PRD",
    "developDEV": "ng build  --configuration=DEV --base-href /anitharestaurant/",
    "developLOCAL": "ng build --configuration=LOCAL --base-href /anitharestaurant/",
    "postbuild": "cpx ./web.config ./anitharestaurant/"
  },
  "lint-staged": {
    "*.{ts,json,scss}": [
      "prettier",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "7.2.9",
    "@angular/cdk": "7.3.5",
    "@angular/common": "7.2.9",
    "@angular/compiler": "7.2.9",
    "@angular/core": "7.2.9",
    "@angular/forms": "7.2.9",
    "@angular/http": "7.2.9",
    "@angular/language-service": "7.2.9",
    "@angular/material": "7.3.5",
    "@angular/material-moment-adapter": "^7.3.5",
    "@angular/platform-browser": "7.2.9",
    "@angular/platform-browser-dynamic": "7.2.9",
    "@angular/platform-server": "7.2.9",
    "@angular/router": "7.2.9",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "angular-font-awesome": "^3.1.2",
    "chart.js": "^2.8.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "momentjs": "^2.0.0",
    "ng2-completer": "1.2.2",
    "ngx-cookie": "^1.0.0",
    "ngx-cookie-service": "^1.0.7",
    "node-sass": "^4.12.0",
    "object-hash": "^1.3.1",
    "rxjs": "^6.4.0",
    "tslib": "^1.9.0",
    "underscore": "^1.8.3",
    "xlsx": "^0.14.3",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "7.2.9",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@types/chart.js": "^2.7.48",
    "@types/jasmine": "~2.5.38",
    "@types/node": "~6.0.60",
    "@types/underscore": "^1.8.1",
    "codelyzer": "~2.0.0",
    "husky": "^0.14.3",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "protractor": "~5.1.0",
    "rxjs-compat": "^6.3.3",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "3.2.4"
  }
}
